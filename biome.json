{
  "$schema": "https://biomejs.dev/schemas/1.9.1/schema.json",
  "files": {
    "ignore": ["./src/routeTree.gen.ts"],
    "ignoreUnknown": true
  },
  "vcs": {
    "enabled": true,
    "clientKind": "git",
    "defaultBranch": "main"
  },
  "linter": {
    "rules": {
      "correctness": {
        "noUnusedImports": "warn",
        "noUnusedVariables": "warn"
      },
      "style": {
        "noNonNullAssertion": "warn"
      }
    }
  },
  "formatter": {
    "indentStyle": "space"
  },
  "organizeImports": {
    "enabled": true
  }
}
